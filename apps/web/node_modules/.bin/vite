#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/adam/Dev/magic-template/node_modules/.pnpm/vite@7.1.5_@types+node@20.19.13_jiti@2.5.1_lightningcss@1.30.1/node_modules/vite/bin/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/vite@7.1.5_@types+node@20.19.13_jiti@2.5.1_lightningcss@1.30.1/node_modules/vite/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/vite@7.1.5_@types+node@20.19.13_jiti@2.5.1_lightningcss@1.30.1/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/adam/Dev/magic-template/node_modules/.pnpm/vite@7.1.5_@types+node@20.19.13_jiti@2.5.1_lightningcss@1.30.1/node_modules/vite/bin/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/vite@7.1.5_@types+node@20.19.13_jiti@2.5.1_lightningcss@1.30.1/node_modules/vite/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/vite@7.1.5_@types+node@20.19.13_jiti@2.5.1_lightningcss@1.30.1/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi

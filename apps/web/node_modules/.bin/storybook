#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/adam/Dev/magic-template/node_modules/.pnpm/storybook@9.1.5_@testing-library+dom@10.4.1_msw@2.11.1_@types+node@20.19.13_typescript@5.9.2__jlwu5m3mpedhoudkod3sjhqipm/node_modules/storybook/bin/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/storybook@9.1.5_@testing-library+dom@10.4.1_msw@2.11.1_@types+node@20.19.13_typescript@5.9.2__jlwu5m3mpedhoudkod3sjhqipm/node_modules/storybook/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/storybook@9.1.5_@testing-library+dom@10.4.1_msw@2.11.1_@types+node@20.19.13_typescript@5.9.2__jlwu5m3mpedhoudkod3sjhqipm/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/adam/Dev/magic-template/node_modules/.pnpm/storybook@9.1.5_@testing-library+dom@10.4.1_msw@2.11.1_@types+node@20.19.13_typescript@5.9.2__jlwu5m3mpedhoudkod3sjhqipm/node_modules/storybook/bin/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/storybook@9.1.5_@testing-library+dom@10.4.1_msw@2.11.1_@types+node@20.19.13_typescript@5.9.2__jlwu5m3mpedhoudkod3sjhqipm/node_modules/storybook/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/storybook@9.1.5_@testing-library+dom@10.4.1_msw@2.11.1_@types+node@20.19.13_typescript@5.9.2__jlwu5m3mpedhoudkod3sjhqipm/node_modules:/home/adam/Dev/magic-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../storybook/bin/index.cjs" "$@"
else
  exec node  "$basedir/../storybook/bin/index.cjs" "$@"
fi
